{
  "name": "@mr-shoaib/ms-ui",
  "version": "0.0.2",
  "description": "ui library for building reusable and flexible  component",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "src/index.js",
  "module": "dist/esm/index.js",
  "types": "src/types/index.d.ts",
  "style": "src/index.css",
  "files": [
    "dist/**/*"
  ],
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": 100,
            "safari": 15,
            "firefox": 91
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MSMALIK11/ms-ui.git"
  },
  "author": "MR SHOAIB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MSMALIK11/ms-ui/issues"
  },
  "homepage": "https://github.com/MSMALIK11/ms-ui#readme",
  "postcss": {
    "plugins": {
      "postcss-import": {}
    }
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.25",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/addon-webpack5-compiler-swc": "1.0.2",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-webpack5": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@types/mime": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@rollup/plugin-image": "^3.0.3",
    "@storybook/addon-postcss": "^2.0.0",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "yup": "^1.4.0"
  }
}
